{
  "name": "externo",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "prepare": "is-ci || husky install",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules --no-notify src/app.ts",
    "test": "jest --transformIgnorePatterns \"node_modules/\" --coverage --passWithNoTests"
  },
  "repository": "https://github.com/vaidebike/Externo.git",
  "author": "VÃ­tor Barroso <contact@owozsh.dev>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@faker-js/faker": "^7.6.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
